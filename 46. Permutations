class Solution {
    int[] num;
    List<List<Integer>> output = new ArrayList<>();
    public List<List<Integer>> permute(int[] nums) {
        this.num = nums;
        track(new ArrayList<Integer>());
        return output;
    }
    public void track(List<Integer> curr){
        if(curr.size() == num.length){
            output.add(new ArrayList<Integer>(curr));
        }
        else{
            for(int i=0; i<num.length; i++){
                if(curr.contains(num[i])){
                    continue;
                }
                curr.add(num[i]);
                track(curr);
                curr.remove(curr.size()-1);
            }
        }
    }
}
